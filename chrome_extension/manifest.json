{
	"name": "Slackify Github Pull Requests",
	"version": "0.1",
	"manifest_version": 2,
	"description": "",
	"update_url": "http://0.0.0.0",
	"permissions": [
		"activeTab",
		"http://*/*",
		"https://*/*"
	],
	"icons": {
		"16": "images/icon-16.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"browser_action": {
		"default_icon": {
			"48": "images/icon-48.png"
		},
		"default_title": "Slackify Github Pull Requests",
		"default_popup": "popup.html"
	}
}